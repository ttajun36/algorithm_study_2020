import java.util.*;

class  Q2852
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int[][] score = new int [n+1][3];
		String s;
		for (int i=0;i<n ;i++ ){
			score[i][0] = sc.nextInt();
			s=sc.next();
			score[i][1] = Integer.parseInt(s.substring(0,2));		//ºÐ
			score[i][2] = Integer.parseInt(s.substring(3,5));		//ÃÊ
		}
		score[n][1] = 48;	score[n][2] = 0;
		
		int[][] ansTeam = new int[2][3];	//team[0][0]Àº 1ÆÀ ºÐ, [0][1]Àº 1ÆÀ ÃÊ, [0][2] ´Â 1ÆÀ Á¡¼ö
		int winningTeam = -1;

		for (int i=0;i<n ;i++ ){
			ansTeam[score[i][0]-1][2]++;
			if (ansTeam[0][2] > ansTeam[1][2])
				winningTeam = 0;
			else if (ansTeam[0][2] < ansTeam[1][2])
				winningTeam = 1;
			else
				winningTeam = -1;

			if (winningTeam != -1){
				ansTeam[winningTeam][0] += (score[i+1][1] - score[i][1]);
				ansTeam[winningTeam][1] += (score[i+1][2] - score[i][2]);
			}
		}

		for (int i=0;i<2 ;i++ ){
			if (ansTeam[i][1]<0){
				ansTeam[i][0] -= (-ansTeam[i][1]/60 + 1);
				ansTeam[i][1] += ((-ansTeam[i][1]/60 + 1)*60);
			}
			if (ansTeam[i][1]>60){
				ansTeam[i][0] += ansTeam[i][1]/60;
				ansTeam[i][1] %= 60;
			}
		}

		String[] ans = new String[2];
		ans[0] = ""; ans[1]="";
		for (int i=0;i<2 ;i++ )
			for (int j=0;j<2 ;j++ ){
				if (ansTeam[i][j]/10 == 0)
					ans[i] += "0";
				ans[i] += ansTeam[i][j];

				if (j!=1)
					ans[i] +=":";
			}

		System.out.println(ans[0]);
		System.out.println(ans[1]);
	}
}
