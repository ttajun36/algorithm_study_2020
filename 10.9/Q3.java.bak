import java.util.*;

class Q3 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for (int i=0;i<t ;i++ )
		{
			int n = sc.nextInt();

			int x = (int)Math.sqrt(n);

			if (x*x == n)
				System.out.println(2*x-2);
			else if((x+1)*x >= n)
				System.out.println(2*x-1);
			else
				System.out.println(2*x + "this");
		}
	}
}
